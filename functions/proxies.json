{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "proxy_root_slash": {
            "matchCondition": {
                "route": "/"
            },
            "backendUri": "https://%ASSET_HOST%/index.html"
        },
        "proxy_index_html": {
            "matchCondition": {
                "route": "/index.html"
            },
            "backendUri": "https://%ASSET_HOST%/index.html"
        },
        "proxy_index_manifest": {
            "matchCondition": {
                "route": "/manifest.json"
            },
            "backendUri": "https://%ASSET_HOST%/manifest.json"
        },
        "proxy_index_asset_manifest": {
            "matchCondition": {
                "route": "/asset-manifest.json"
            },
            "backendUri": "https://%ASSET_HOST%/asset-manifest.json"
        },
        "proxy_index_asset_service_worker": {
            "matchCondition": {
                "route": "/service-worker.js"
            },
            "backendUri": "https://%ASSET_HOST%/service-worker.js"
        },
        "proxy_index_asset_favicon": {
            "matchCondition": {
                "route": "/favicon.ico"
            },
            "backendUri": "https://%ASSET_HOST%/favicon.ico"
        },
        "proxy_static_css_files": {
            "matchCondition": {
                "route": "/static/css/{*path}"
            },
            "backendUri": "https://%ASSET_HOST%/static/css/{path}"
        },
        "proxy_static_js_files": {
            "matchCondition": {
                "route": "/static/js/{*path}"
            },
            "backendUri": "https://%ASSET_HOST%/static/js/{path}"
        },
        "proxy_static_media_files": {
            "matchCondition": {
                "route": "/static/media/{*path}"
            },
            "backendUri": "https://%ASSET_HOST%/static/media/{path}"
        }
    }
}
