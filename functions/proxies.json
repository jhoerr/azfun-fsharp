{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "api": {
            "matchCondition": {
                "route": "/api/{*url}"
            },
            "backendUri": "https://%WEBSITE_HOSTNAME%/api/{url}"
        },
        "spaIndex": {
            "matchCondition": {
                "route": "/index.html"
            },
            "backendUri": "https://%SPA_HOST%/app/index.html"
        },
        "spaJs": {
            "matchCondition": {
                "route": "/static/js/main.0793009d.js"
            },
            "backendUri": "https://%SPA_HOST%/app/static/js/main.0793009d.js"
        },
        "spaCss": {
            "matchCondition": {
                "route": "/static/css/main.06a917e2.css"
            },
            "backendUri": "https://%SPA_HOST%/app/static/css/main.06a917e2.css"
        }
    }
}
